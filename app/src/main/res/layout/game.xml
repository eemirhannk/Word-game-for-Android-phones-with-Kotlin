            android:layout_height="wrap_content"
            android:text="0"
            android:textAlignment="center"
            android:gravity="center"
            android:textColor="#0ef"
            android:textSize="50sp"
            android:textStyle="bold"
            tools:ignore="HardcodedText" />
    </GridLayout>

